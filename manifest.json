{
    "name": "Clear Skies",
    "description": "Small weather widget, drawing on weather.gov's API for info and terrible code to make it readable.",
    "version": "0.8",
    "permissions": [ "storage", "geolocation" ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/16x.png",
      "32": "icons/32x.png",
      "48": "icons/48x.png",
      "128": "icons/128x.png"
    },
    "manifest_version": 3
  }
